<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <import resource="DataProviderFilesConfig.xml" />

    <!-- Default location of ThreadRunnerConfig file! -->
    <bean id="ThreadRunnerConfigProviderPathString" class="java.lang.String" c:_0="conf/threadRunnerConf.json"/>

    <!-- Default location of Sources file! -->
    <bean id="ThreadRunnerSourceProviderPathString" class="java.lang.String" c:_0="conf/threadRunnerSources.json"/>

    <!-- Default location of Factory configuration file file! -->
    <bean id="ThreadRunnerFactoryConfigProviderPathString" class="java.lang.String" c:_0="conf/dataNukeDefaultFactoryConf.json"/>
</beans>